            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import io.jsonwebtoken.Jwts;
import io.jsonwebtoken.SignatureAlgorithm;
import java.time.Instant;
import java.time.temporal.ChronoUnit;

// Define the payload values
def issuer = &quot;e1aaab8a76f33c54e1a0ea93b1e88b1f186d39f3021471b5c6c863880966e587&quot;
def subject = &quot;dtonymorello9100@gmail.com&quot;
def expirationDurationMinutes = 15 // Set the expiration duration in minutes

// Calculate the expiration time
def expirationTime = Instant.now().plus(expirationDurationMinutes, ChronoUnit.MINUTES).getEpochSecond()

// Generate the bearer token
def bearerToken = Jwts.builder()
    .setIssuer(issuer)
    .setSubject(subject)
    .setExpiration(new Date(expirationTime * 1000))
    .signWith(SignatureAlgorithm.HS256, &quot;f18761906029632a9ded1a2d93211187b8ed20ce2aad2e680cf954686e3fd0bd&quot;) // Replace &quot;your-secret-key&quot; with your actual secret key
    .compact()

// Use the bearer token in your test script
log.info(&quot;Generated Bearer Token: $bearerToken&quot;)

// Update the value of the &quot;jwtToken&quot; variable
vars.put(&quot;jwtToken&quot;, &quot;Bearer &quot; + bearerToken)
</stringProp>
